}

QDateEdit:hover {
    border-color: #2962FF;
    background-color: rgb(35, 37, 40);
}

QDateEdit:focus {
    border: 1px solid #2962FF;
    outline: none;
}

QDateEdit::drop-down {
    subcontrol-origin: padding;
    subcontrol-position: top right;
    width: 20px;
    border: none;
    border-left: 1px solid #50555e;
    border-top-right-radius: 4px;
    border-bottom-right-radius: 4px;
    background-color: rgba(50, 54, 60, 30);
}

QDateEdit::drop-down:hover {
    background-color: rgba(50, 54, 60, 60);
}

QDateEdit::down-arrow {
    image: url(:/icon_gris/calendar.svg);
    width: 12px;
    height: 12px;
}

/* ===============================================
   BARRES DE DÉFILEMENT
   =============================================== */

QScrollBar:vertical {
    border: none;
    background-color: rgba(30, 32, 35, 0);
    width: 10px;
    margin: 0;
}

QScrollBar:horizontal {
    border: none;
    background-color: rgba(30, 32, 35, 0);
    height: 10px;
    margin: 0;
}

QScrollBar::handle:vertical,
QScrollBar::handle:horizontal {
    background-color: rgba(50, 54, 60, 50);
    border-radius: 4px;
    min-height: 20px;
    min-width: 20px;
    opacity: 0;
    transition: opacity 300ms ease, background-color 300ms ease;
}

QScrollBar::handle:vertical:hover,
QScrollBar::handle:horizontal:hover {
    background-color: rgba(41, 98, 255, 150);
    opacity: 1;
}

QScrollBar::handle:vertical:pressed,
QScrollBar::handle:horizontal:pressed {
    background-color: rgba(21, 74, 201, 200);
}

QScrollBar::add-line:vertical,
QScrollBar::sub-line:vertical,
QScrollBar::add-line:horizontal,
QScrollBar::sub-line:horizontal {
    border: none;
    background: none;
    height: 0;
    width: 0;
}

QScrollBar:vertical:hover,
QScrollBar:horizontal:hover {
    background-color: rgba(30, 32, 35, 30);
}

QScrollBar::add-page:vertical,
QScrollBar::sub-page:vertical,
QScrollBar::add-page:horizontal,
QScrollBar::sub-page:horizontal {
    background: none;
}

